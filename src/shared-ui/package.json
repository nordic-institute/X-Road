{
  "name": "@niis/shared-ui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint . --fix",
    "build": "pnpm run generate-types",
    "generate-types": "rm -rf src/openapi-types && openapi --input ../common/common-admin-api/src/main/resources/common-openapi-definition.yaml --output ./src/openapi-types --exportCore false --exportServices false",
    "license-check": "license-check-and-add check -f license-check-and-add-config.json",
    "license-add": "license-check-and-add add -f license-check-and-add-config.json",
    "cleanup": "rm -rf dist && rm -rf node_modules",
    "format": "prettier .  --write"
  },
  "main": "./src/index.ts",
  "dependencies": {
    "@fontsource/open-sans": "^5.2.7",
    "@mdi/font": "^7.4.47",
    "@vee-validate/i18n": "~4.15.1",
    "@vee-validate/rules": "~4.15.1",
    "axios": "~1.12.2",
    "dayjs": "^1.11.18",
    "deepmerge": "^4.3.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3",
    "vuetify": "3.10.5"
  },
  "engines": {
    "npm": "use-pnpm",
    "yarn": "use-pnpm",
    "node": ">=22 <=24",
    "pnpm": ">=10"
  }
}
