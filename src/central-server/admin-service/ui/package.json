{
  "name": "xroad-centralserver-admin-ui",
  "version": "7.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "rm -rf src/openapi-types && npm run generate-types && vite build",
    "build:gradle": "vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "npx-check-audit": "yarn dlx audit-ci@6.6.0 --config ../../../shared-ui/audit-ci-shared.json >build/reports/audit-ci.txt",
    "license-check": "license-check-and-add check -f license-check-and-add-config.json",
    "license-add": "license-check-and-add add -f license-check-and-add-config.json",
    "bundle-openapi": "swagger-cli bundle -t yaml -o ./build/openapi/openapi-definition-bundled.yaml ../../openapi-model/src/main/resources/openapi-definition.yaml",
    "cleanup": "rm -rf dist && rm -rf yarn.lock && rm -rf node_modules",
    "generate-types": "npm run bundle-openapi && openapi --input ./build/openapi/openapi-definition-bundled.yaml --output ./src/openapi-types --exportCore false --exportServices false",
    "resolve-audit": "resolve-audit"
  },
  "dependencies": {
    "@fontsource/open-sans": "^5.0.12",
    "@mdi/font": "^7.2.96",
    "@niis/shared-ui": "file:../../../shared-ui/niis-shared-ui-3.0.0.tgz",
    "@vee-validate/i18n": "~4.11.6",
    "@vee-validate/rules": "~4.11.6",
    "axios": "~1.4.0",
    "dayjs": "^1.11.10",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vee-validate": "~4.11.6",
    "vue": "^3.3.4",
    "vue-i18n": "~9.4.1",
    "vue-router": "^4.2.5",
    "vuetify": "^3.3.19"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "~4.0.4",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.17.8",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuetify": "^2.0.5",
    "license-check-and-add": "~4.0.5",
    "npm-run-all": "^4.1.5",
    "openapi-types": "^12.1.3",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^1.8.8"
  },
  "packageManager": "yarn@4.0.2"
}
