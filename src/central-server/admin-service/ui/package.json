{
  "name": "xroad-centralserver-admin-ui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "pnpm run generate-types && vitest --run &&  vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "test": "vitest",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "license-check": "license-check-and-add check -f ../../../shared-ui/license-check-and-add-config.json",
    "license-add": "license-check-and-add add -f ../../../shared-ui/license-check-and-add-config.json",
    "generate-types": "rm -rf src/openapi-types && openapi --input ../../openapi-model/src/main/resources/openapi-definition.yaml --output ./src/openapi-types --exportCore false --exportServices false"
  },
  "dependencies": {
    "@niis/shared-ui": "workspace:*",
    "is-ip": "^5.0.1"
  },
  "devDependencies": {
    "openapi-types": "^12.1.3"
  },
  "engines": {
    "npm": "use-pnpm",
    "yarn": "use-pnpm",
    "node": ">=22 <=24",
    "pnpm": ">=10"
  }
}
