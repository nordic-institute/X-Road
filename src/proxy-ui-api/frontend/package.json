{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "rm -rf src/openapi-types && npm run generate-types && vue-cli-service serve",
    "build": "rm -rf src/openapi-types && npm run generate-types && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:report": "vue-cli-service build --report",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test:coverage": "vue-cli-service test:unit --coverage",
    "npx-check-audit": "npx audit-ci@6.3.0 --config ../../shared-ui/audit-ci-shared.json >../build/reports/audit-ci.txt",
    "generate-types": "openapi --input ../src/main/resources/openapi-definition.yaml --output ./src/openapi-types --exportCore false --exportServices false",
    "license-check": "license-check-and-add check -f license-check-and-add-config.json",
    "license-add": "license-check-and-add add -f license-check-and-add-config.json",
    "install-ui-lib": "cd ../../shared-ui && npm install",
    "build-ui-lib": "cd ../../shared-ui && npm run build-lib",
    "install-build-ui-lib": "cd ../../shared-ui && npm install && npm run build-lib",
    "audit-check-ui-lib": "cd ../../shared-ui && npm run npx-check-audit",
    "license-check-ui-lib": "cd ../../shared-ui && npm run license-check",
    "serve-only": "vue-cli-service serve",
    "build-all": "npm run install-build-ui-lib && npm install && npm run build",
    "cleanup": "rm -rf dist && rm -rf package-lock.json && rm -rf node_modules"
  },
  "dependencies": {
    "@fontsource/open-sans": "~4.5.0",
    "@niis/shared-ui": "file:../../shared-ui/niis-shared-ui-0.1.0.tgz",
    "@vue/composition-api": "^1.4.6",
    "axios": "^0.26.0",
    "core-js": "~3.21.1",
    "pinia": "2.0.12",
    "pinia-plugin-persistedstate": "^1.2.1",
    "vee-validate": "3.4.14",
    "vue": "2.6.14",
    "vue-i18n": "~8.26.0",
    "vue-router": "~3.5.1",
    "vuetify": "^2.6.10"
  },
  "devDependencies": {
    "@mdi/font": "~6.5.95",
    "@types/jest": "^27.0.0",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "@vue/cli-plugin-babel": "~4.5.16",
    "@vue/cli-plugin-e2e-nightwatch": "~4.5.16",
    "@vue/cli-plugin-eslint": "~4.5.16",
    "@vue/cli-plugin-typescript": "~4.5.16",
    "@vue/cli-plugin-unit-jest": "~4.5.16",
    "@vue/cli-service": "~4.5.16",
    "@vue/eslint-config-prettier": "~6.0.0",
    "@vue/eslint-config-typescript": "~7.0.0",
    "@vue/test-utils": "~1.2.0",
    "chromedriver": "^105.0.1",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "~3.4.0",
    "eslint-plugin-vue": "~7.20.0",
    "eslint-plugin-vue-scoped-css": "^1.3.0",
    "license-check-and-add": "~4.0.2",
    "openapi-typescript-codegen": "^0.16.2",
    "prettier": "~2.4.1",
    "sass": "~1.32.6",
    "sass-loader": "~10.1.1",
    "typescript": "4.4.4",
    "vue-cli-plugin-vuetify": "~2.4.1",
    "vue-template-compiler": "2.6.14",
    "vuetify-loader": "~1.7.3"
  },
  "overrides": {
    "trim": "1.0.1",
    "trim-newlines": "4.0.2",
    "glob-parent": "^5.1.2",
    "minimatch": "^3.1.2",
    "flat": "^5.0.1",
    "loader-utils": "^2.0.4"
  }
}
