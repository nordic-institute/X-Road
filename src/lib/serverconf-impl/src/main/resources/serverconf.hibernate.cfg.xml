<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
    "-//Hibernate/Hibernate Configuration DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
    <session-factory>
        <property name="hikari.maximumPoolSize">20</property>
        <property name="hibernate.id.db_structure_naming_strategy">single</property>
        <mapping class="org.niis.xroad.serverconf.entity.AccessRightEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.CertificateEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.ClientIdEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.ClientEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.EndpointEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.GlobalGroupIdEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.GroupMemberEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.LocalGroupIdEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.LocalGroupEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.SecurityServerIdEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.ServerConfEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.ServiceDescriptionEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.ServiceIdEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.ServiceEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.TimestampingServiceEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.UiUserEntity" />
        <mapping class="org.niis.xroad.serverconf.entity.XRoadIdEntity" />
    </session-factory>
</hibernate-configuration>
