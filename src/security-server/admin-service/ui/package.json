{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "serve": "rm -rf src/openapi-types && npm run generate-types && vite preview",
    "build": "rm -rf src/openapi-types && npm run generate-types && vite build",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts  --fix --ignore-path .gitignore",
    "npx-check-audit": "yarn dlx audit-ci@6.6.0 --config ../../../shared-ui/audit-ci-shared.json > build/reports/audit-ci.txt",
    "generate-types": "openapi --input ../../openapi-model/src/main/resources/META-INF/openapi-definition.yaml --output ./src/openapi-types --exportCore false --exportServices false",
    "license-check": "license-check-and-add check -f license-check-and-add-config.json",
    "license-add": "license-check-and-add add -f license-check-and-add-config.json",
    "install-ui-lib": "cd ../../../shared-ui && yarn install",
    "build-ui-lib": "cd ../../../shared-ui && npm run build",
    "install-build-ui-lib": "cd ../../../shared-ui && yarn install && npm run build",
    "audit-check-ui-lib": "cd ../../../shared-ui && npm run npx-check-audit",
    "license-check-ui-lib": "cd ../../../shared-ui && npm run license-check",
    "build-all": "npm run install-build-ui-lib && yarn install && npm run build",
    "cleanup": "rm -rf dist && rm -rf yarn.lock && rm -rf node_modules"
  },
  "dependencies": {
    "@fontsource/open-sans": "~4.5.0",
    "@niis/shared-ui": "file:../../../shared-ui/niis-shared-ui-3.0.0.tgz",
    "@vee-validate/i18n": "~4.12.2",
    "@vee-validate/rules": "~4.11.6",
    "axios": "~1.4.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vee-validate": "^4.12.2",
    "vue": "^3.3.4",
    "vue-i18n": "^9.4.0",
    "vue-router": "^4.2.4",
    "vuetify": "^3.3.19"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@mdi/font": "~6.5.95",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/jest": "^27.0.0",
    "@types/node": "^18.16.17",
    "@typescript-eslint/eslint-plugin": "~6.5.0",
    "@typescript-eslint/parser": "~6.5.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-typescript": "~11.0.3",
    "eslint": "~8.48.0",
    "eslint-config-prettier": "~9.0.0",
    "eslint-plugin-prettier-vue": "~5.0.0",
    "eslint-plugin-vue": "~9.17.0",
    "eslint-plugin-vue-scoped-css": "^1.3.0",
    "eslint-plugin-vuetify": "~2.0.5",
    "license-check-and-add": "~4.0.5",
    "npm-run-all": "^4.1.5",
    "openapi-typescript-codegen": "^0.16.2",
    "prettier": "~3.0.3",
    "sass": "~1.33.0",
    "typescript": "~5.0.4",
    "vite": "^4.4.9",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.6.5"
  },
  "packageManager": "yarn@4.0.2"
}
