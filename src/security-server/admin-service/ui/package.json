{
  "name": "xroad-securityserver-admin-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "serve": "rm -rf src/openapi-types && npm run generate-types && vite preview",
    "build": "rm -rf src/openapi-types && npm run generate-types && vite build",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts  --fix --ignore-path .gitignore",
    "generate-types": "openapi --input ../../openapi-model/src/main/resources/META-INF/openapi-definition.yaml --output ./src/openapi-types --exportCore false --exportServices false",
    "license-check": "license-check-and-add check -f license-check-and-add-config.json",
    "license-add": "license-check-and-add add -f license-check-and-add-config.json"
  },
  "dependencies": {
    "@niis/shared-ui": "workspace:*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~6.5.0",
    "@typescript-eslint/parser": "~6.5.0",
    "eslint-plugin-vue-scoped-css": "^1.3.0"
  },
  "engines": {
    "npm": "use-pnpm",
    "yarn": "use-pnpm",
    "node": ">=18 <=20",
    "pnpm": ">=8"
  }
}
