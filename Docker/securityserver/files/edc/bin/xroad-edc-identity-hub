#!/bin/bash

exec java -Dedc.keystore="/etc/xroad-edc/certs/cert.pfx" \
    -Dedc.keystore.password=123456 \
    -Dedc.vault="/etc/xroad-edc/edc-vault.properties" \
    -Dedc.fs.config="/etc/xroad-edc/edc-identity-hub.properties" \
    -Xdebug -agentlib:jdwp=transport=dt_socket,address=*:9954,server=y,suspend=n \
    -Xmx256m -jar /usr/share/xroad/jlib/edc-identity-hub.jar